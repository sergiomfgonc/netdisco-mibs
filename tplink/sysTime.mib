TLSG3424-SYSTIME-MIB DEFINITIONS ::= BEGIN

          IMPORTS
              OBJECT-TYPE,  IpAddress 
          	  	         	FROM SNMPv2-SMI
              
              sysConfig	          
          	  	          	FROM TLSG3424-SYSTEM-MIB;
      	
      	      sysTime		OBJECT IDENTIFIER ::= {sysConfig 3}
 
      	  
      	  sysTimeCurrentTime	   OBJECT-TYPE
      	       SYNTAX      OCTET STRING(SIZE (0..255)) 
	       MAX-ACCESS  read-only
	       STATUS      current
	       DESCRIPTION
	               "the date"
	       ::= { sysTime 1 }
      	  
      	 sysTimeSource			OBJECT-TYPE
      	       SYNTAX      OCTET STRING(SIZE (0..255)) 
	       MAX-ACCESS  read-only
	       STATUS      current
	       DESCRIPTION
	               "the time source"
	       ::= { sysTime 2 }
	       
	  sysTimeMode			OBJECT-TYPE
      	       SYNTAX      OCTET STRING(SIZE (0..255)) 
	       MAX-ACCESS  read-only
	       STATUS      current
	       DESCRIPTION
	               "the time mode"
	       ::= { sysTime 3 }
	       
      	  sysTimeManualTimeConfig		OBJECT IDENTIFIER ::= {sysTime 4}
      	  sysTimeGmtTimeConfig		OBJECT IDENTIFIER ::= {sysTime 5}
      	  --dstConfig	    OBJECT IDENTIFIER ::= {sysTime 6}	
      	  --sysTimePcTimeConfig		OBJECT IDENTIFIER ::= {sysTime 6}

	       
	 sysTimeManualTimeEnable	 OBJECT-TYPE
	       SYNTAX      INTEGER{
	      		disable(0), 						--disable
	      		enable(1)						--enable
	      		}
	       MAX-ACCESS  read-write
	       STATUS      current
	       DESCRIPTION
	               "0.disbale
	               	1.enable"
	       ::= { sysTimeManualTimeConfig 1 }
      	  
      	  sysTimeDate	 	OBJECT-TYPE
	       SYNTAX      OCTET STRING
	       MAX-ACCESS  read-write
	       STATUS      current
	       DESCRIPTION
	               "the data the set"
	       ::= { sysTimeManualTimeConfig 2 }
	       
	  sysTimeGmtTimeEnable	 OBJECT-TYPE
	       SYNTAX      INTEGER{
	      		disable(0), 						--disable
	      		enable(1)						--enable
	      		}
	       MAX-ACCESS  read-write
	       STATUS      current
	       DESCRIPTION
	               "0.disbale
	               	1.enable"
	       ::= { sysTimeGmtTimeConfig 1 }	
	       
	   sysTimeGmtTimezone	 OBJECT-TYPE
	       SYNTAX      INTEGER{
							utc-1200(-1200),
							utc-1100(-1100), 
							utc-1000(-1000), 
							utc-0900(-900),
							utc-0800(-800),
